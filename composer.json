{
  "description": "A Neos CMS package which makes the Form Builder Bootstrap 5 compatible.",
  "type": "neos-package",
  "name": "neosrulez/bootstrap-formbuilder",
  "require": {
    "neos/neos": "*",
    "neosrulez/bootstrap": "^5.0",
    "neos/form-builder": "^2.2",
    "neosrulez/form-multicolumnsection": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "NeosRulez\\Bootstrap\\FormBuilder\\": "Classes/"
    }
  },
  "extra": {
    "neos": {
      "package-key": "NeosRulez.Bootstrap.FormBuilder"
    }
  }
}
